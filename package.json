{
  "name": "al-barrage-datamine",
  "version": "1.0.0",
  "description": "Azur Lane barrage data mining and processing tool",
  "type": "module",
  "private": true,
  "main": "src/index.ts",
  "module": "src/index.ts",
  "scripts": {
    "start": "bun run barrage && bun run wiki",
    "barrage": "bun run src/barrage/index.ts && bun run lint",
    "wiki": "bun run src/wiki/index.ts",
    "test": "bun test",
    "lint": "biome check --write .",
    "update": "bun update --latest && git submodule update --init --recursive",
    "prepare": "bunx lefthook install"
  },
  "dependencies": {
    "mwn": "^3.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "@types/bun": "^1.3.0",
    "lefthook": "^1.13.6"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  }
}
